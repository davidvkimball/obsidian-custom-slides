{
	"name": "custom-slides",
	"version": "1.0.3",
	"description": "Customizes the Slides core plugin navigation, styles, and behavior.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "esbuild main.ts --bundle --outfile=main.js --sourcemap --external:obsidian --external:*.css",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["slides"],
	"author": "David V. Kimball",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
